{
	"fileInfo": {
	  "prefix": "info",
	  "body": [
		"<!-----------------------------------------------------------------------------",
		"   Program Name        :        $1",
		"   Author              :        $2",
		"   Input               :        $3",
		"   Output              :        $4",
		"   Return              :        $5",
		"   Function            :        $6",
		"   Update              :        $7  $8  $9",
		"------------------------------------------------------------------------------>"
	  ],
	  "description": "Program Name, Author"
	},
