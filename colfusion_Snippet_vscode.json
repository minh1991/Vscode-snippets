// JAVASCRIPT
  
	// COMMON
	"jsConsoleLog": {
	  "prefix": "js-clg",
	  "body": ["console.log('$1');"],
	  "description": "JAVASCRIPT.consoleLog"
	},

	"jsAlert": {
		"prefix": "js-alert",
		"body": [
			"alert('$1');"
		],
		"description": "JAVASCRIPT.alert"
	  },
  
	"jsFunction": {
	  "prefix": "js-function",
	  "body": [
		"function $1 ($2) {",
		"     $3",
		"};"
	  ],
	  "description": "JAVASCRIPT.function"
	},

	//VARIABLE
	"jsVar": {
		"prefix": "js-var",
