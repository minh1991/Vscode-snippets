	// IF - ELSE - CONDITION
    "jsIf": {
        "prefix": "js-if",
        "body": [
          "if ($1) {",
          "   $2",
          "}"
        ],
        "description": "JAVASCRIPT.if"
     },
	 "jsIf-else": {
        "prefix": "js-if-else",
        "body": [
			"if ($1) {",
			"   $2",
			"} else {",
			"   $3",
			"};"
        ],
        "description": "JAVASCRIPT.if-else"
	 },
	 "jsElse-if": {
        "prefix": "js-else-if",
        "body": [
			"if ($1) {",
			"   $2",
			"} else if ($3) {",
			"   $4",
			"} else {",
			"   $5",
			"};"
        ],
        "description": "JAVASCRIPT.else-if"
	 },
	 
	 "jsSwitch-case": {
        "prefix": "js-switch-case",
        "body": [
			"switch ($1) {",
			"    case $2:",
			"        $3",
			"        break;",
			"    case $4:",
			"        $5",
			"        break;",
			"    case $6:",
			"        $7",
			"        break;",
			"    default:",
			"        $8",
			"};"
        ],
        "description": "JAVASCRIPT.switch-case"
	 }



