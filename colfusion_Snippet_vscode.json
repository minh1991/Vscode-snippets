{
	"fileInfo": {
	  "prefix": "info",
	  "body": [
		"<!-----------------------------------------------------------------------------",
		"   Program Name        :        $1",
		"   Author              :        $2",
		"   Input               :        $3",
		"   Output              :        $4",
		"   Return              :        $5",
		"   Function            :        $6",
		"   Update              :        $7  $8  $9",
		"------------------------------------------------------------------------------>"
	  ],
	  "description": "Program Name, Author"
	},
	"htmlCreate": {
	  "prefix": "ht-!",
	  "body": [
		"<!DOCTYPE HTML PUBLIC '-//W3C//DTD HTML 4.0 Transitional//EN'>",
		"<HTML>",
		"     <HEAD>",
		"          <META http-equiv='Content-Type' content='text/html; charset=Shift_JIS'>",
		"          <TITLE>$1</TITLE>",
		"          <SCRIPT type='text/javascript'>",
		"          </SCRIPT>",
		"          <STYLE TYPE='text/css'>",
		"          </STYLE>",
		"     </HEAD>",
		"     <BODY>",
		"     </BODY>",
		"</HTML>"
	  ],
	  "description": "HTML"
	},
  
	"htmlScript": {
	  "prefix": "ht-script",
	  "body": [
		"<SCRIPT>$1</SCRIPT>"
	  ],
	  "description": "HTML.script"
	},
  
  
  
  
  
  
